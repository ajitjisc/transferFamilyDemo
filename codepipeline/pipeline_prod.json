{
    "pipeline": {
        "name": "TransferFamilyPipelineProd",
        "roleArn": "arn:aws:iam::492883160621:role/CodePipelineRole",
        "artifactStore": {
            "type": "S3",
            "location": "demo-transfam-pipeline-artifacts-bucket"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "Source",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "AWS",
                            "provider": "CodeStarSourceConnection",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "BranchName": "feature/den-test",
                            "ConnectionArn": "arn:aws:codestar-connections:eu-west-2:492883160621:connection/c5ac251e-4e89-4e22-be0b-10e48f21a273",
                            "DetectChanges": "true",
                            "FullRepositoryId": "ajitjisc/transferFamilyDemo"
                        },
                        "outputArtifacts": [
                            {
                                "name": "source_output"
                            }
                        ],
                        "inputArtifacts": [],
                        "namespace": "SourceVariables"
                    }
                ]
            },
            {
                "name": "Terraform",
                "actions": [
                    {
                        "name": "TerraformBuild",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "EnvironmentVariables": "[{\"name\":\"ENV\",\"type\":\"PLAINTEXT\",\"value\":\"#{SourceVariables.BranchName}\"}]",
                            "ProjectName": "TerraformBuildProject"
                        },
                        "outputArtifacts": [
                            {
                                "name": "build_output"
                            }
                        ],
                        "inputArtifacts": [
                            {
                                "name": "source_output"
                            }
                        ]
                    }
                ]
            },
            {
                "name": "DeployServerless",
                "actions": [
                    {
                        "name": "ServerlessDeploy",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "EnvironmentVariables": "[{\"name\":\"ENV\",\"type\":\"PLAINTEXT\",\"value\":\"#{SourceVariables.BranchName}\"}]",
                            "ProjectName": "ServerlessBuildProject"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": [
                            {
                                "name": "build_output"
                            }
                        ]
                    }
                ]
            }
        ],
        "version": 3,
        "executionMode": "SUPERSEDED",
        "pipelineType": "V1"
    }
}
